{
  "name": "@sarahbonzelet.de/website",
  "version": "0.2.0",
  "dependencies": {
    "autoprefixer": "10.4.2",
    "bulma": "0.9.3",
    "postcss-cli": "9.1.0",
    "validator": "13.6.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "10.2.4",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "prettier-plugin-go-template": "0.0.11",
    "webpack": "5.69.1",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "scripts": {
    "build:production": "webpack --mode=production && hugo --minify",
    "build": "webpack --mode=development && hugo --config config-dev.toml",
    "watch": "webpack --mode=development --watch",
    "start": "hugo serve",
    "precommit": "webpack --mode=production && lint-staged"
  }
}
